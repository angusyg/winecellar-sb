{
  "name": "winecellar-client",
  "version": "0.0.0",
  "description": "Winecellar application client",
  "license": "MIT",
  "author": "Angusyg",
  "private": true,
  "scripts": {
    "build": "ng build --prod",
    "format": "npm run format:src && npm run format:tests",
    "format:src": "prettier --write \"src/**/*.{ts,js,html,scss}\"",
    "format:tests": "prettier --write \"e2e/**/*.{ts,js}\"",
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint && npm run lint:sass",
    "lint:sass": "sass-lint -v ./src/**/*.scss",
    "lint:sass:fix": "sass-lint-auto-fix ./src/**/*.scss",
    "test": "ng test",
    "test:e2e": "ng e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angusyg/winecellar-client.git"
  },
  "keywords": [
    "angular",
    "typescript",
    "sass",
    "jasmine",
    "protractor",
    "karma"
  ],
  "bugs": {
    "url": "https://github.com/angusyg/winecellar-client/issues"
  },
  "homepage": "https://github.com/angusyg/winecellar-client#readme",
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@ng-bootstrap/ng-bootstrap": "^4.0.1",
    "bootstrap": "^4.2.1",
    "core-js": "^2.6.2",
    "ngx-toastr": "^9.1.1",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.27"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.1",
    "@angular/cli": "~7.2.1",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@types/jasmine": "~3.3.5",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~10.12.18",
    "codelyzer": "~4.5.0",
    "husky": "^1.3.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "protractor": "~5.4.2",
    "sass-lint": "^1.12.1",
    "sass-lint-auto-fix": "^0.15.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "~3.2.2"
  }
}
